{
    "name": "fgilio/simple-laravel-tags",
    "description": "Forked from spatie/laravel-tags, without translation capabilities and with some extra query scopes",
    "keywords": [
        "fgilio",
        "simple-laravel-tags"
    ],
    "homepage": "https://github.com/fgilio/simple-laravel-tags",
    "license": "MIT",
    "authors": [
        {
            "name": "Franco Gilio",
            "email": "fgilio@publica.la",
            "homepage": "https://publica.la",
            "role": "CTO"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.8",
        "spatie/eloquent-sortable": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Fgilio\\Tags\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Fgilio\\Tags\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fgilio\\Tags\\TagsServiceProvider"
            ]
        }
    }
}
